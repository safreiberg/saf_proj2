<table border="1">
    <tr>
      <th>Product ID</th>
      <th>User ID</th>
      <th>ID</th>
      <th>Product Quantity</th>
      <th>Price</th>
      <th>Submitted</th>
    </tr>
    <% Order.find(:all).each do |order| %>
    <tr>
      <td><%= order.product_id %></td>
      <td><%= order.user_id %></td>
      <td><%= order.id %></td>
      <td><%= order.product_quantity %></td>
      <td><%= order.price %></td>
      <td><%= order.created_at %></td>
    </tr><% end %>
  </table>